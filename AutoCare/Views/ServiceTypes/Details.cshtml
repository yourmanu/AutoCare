@model AutoCare.Models.ServiceType
@{
    ViewData["Title"] = "Details";
}

<h2>Detail of Service Type</h2>
    <div class="form-border">
        <div class="form-group">
            <div class="row">
                <div class="col-sm-1">
                    <label asp-for="Name" class="custom-label"></label>
                </div>
                <div class="col-sm-4">
                    <input asp-for="Name" disabled class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <a asp-action="Edit" asp-route-id="@Model.ServicetypeId" class="btn btn-sm btn-success">Edit</a>
            <a asp-action="Index" class="btn btn-sm btn-success">Back to List</a>
        </div>
    </div>


